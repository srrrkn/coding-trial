{
	"name": "Python 3.12",
	"image": "python3.12:latest",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.debugpy",
				"oderwat.indent-rainbow",
				"GitHub.copilot",
				"GitHub.copilot-chat"
			]
		}
	},
	"containerEnv": {
		"PYTHONPATH": "${containerWorkspaceFolder}/works/python3.12/src"
	}
}
